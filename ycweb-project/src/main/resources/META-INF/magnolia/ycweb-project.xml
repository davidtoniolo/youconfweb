<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
	<name>ycweb-project</name>
	<displayName>${project.name}</displayName>
	<description>${project.description}</description>
	<class>com.unittestcloud.ycweb.project.YcwebProjectModule</class>
	<versionHandler>com.unittestcloud.ycweb.project.setup.YcwebProjectModuleVersionHandler</versionHandler>
	<version>${project.version}</version>

	<dependencies>
		<dependency>
			<name>core</name>
			<version>5.1.1/*</version>
		</dependency>
		<dependency>
			<name>rendering</name>
			<version>5.1.1/*</version>
		</dependency>
		<dependency>
			<name>templating</name>
			<version>5.1.1/*</version>
		</dependency>
		<dependency>
			<name>templating-jsp</name>
			<version>5.1.1/*</version>
		</dependency>

		<dependency>
			<name>blossom</name>
			<version>3.0/*</version>
		</dependency>
	</dependencies>

	<repositories>
		<repository>
			<name>magnolia</name>
			<workspaces>
				<workspace>ycwebproject</workspace>
			</workspaces>
		</repository>
	</repositories>
</module>